{
  "buildCommand": "cd cursor/client && npm ci && npm run build",
  "outputDirectory": "cursor/client/dist",
  "installCommand": "cd cursor/client && npm ci",
  "framework": "vite",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

